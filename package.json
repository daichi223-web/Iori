{
  "name": "ai-dev-system",
  "version": "3.0.0",
  "description": "Iori v3.0 - Autonomous AI Development OS with Multi-Brain Architecture",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:pkg": "tsc -p tsconfig.pkg.json",
    "dev": "tsx watch src/index.ts",
    "exe": "npm run build:pkg && pkg dist-pkg/index.js --targets node18-win-x64 --output iori.exe",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "typecheck": "tsc --noEmit",
    "dashboard": "tsx src/frontend/server.ts",
    "orchestrator": "tsx orchestrator/index.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.ts\""
  },
  "keywords": [
    "ai",
    "multi-agent",
    "autonomous",
    "claude",
    "zeami",
    "tdd",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.5.10",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.55.0",
    "pkg": "^5.8.1",
    "prettier": "^3.1.0",
    "supertest": "^6.3.4",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@octokit/plugin-retry": "^6.0.1",
    "@octokit/plugin-throttling": "^8.1.3",
    "@octokit/rest": "^20.0.2",
    "bottleneck": "^2.19.5",
    "chalk": "^5.6.2",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "openai": "^6.14.0",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "simple-git": "^3.22.0",
    "systeminformation": "^5.27.14",
    "unified": "^11.0.5",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
